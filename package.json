{
  "name": "status-board",
  "version": "0.0.0-development",
  "description": "Status Board is a dashboard framework written in nodejs.",
  "scripts": {
    "start": "node ./bin/www",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "clean": "rimraf lib coverage",
    "commit": "git-cz",
    "commitmsg": "opt --in commit-msg --exec \"commitlint -e $GIT_PARAMS\"",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "coverage:report": "coveralls",
    "precommit": "lint-staged && opt --in pre-commit --exec \"yarn verify\"",
    "semantic-release": "semantic-release",
    "setup": "npm i -g npmvet david nsp depcheck types-checker commitizen semantic-release-cli all-contributors-cli",
    "test": "jest --coverage",
    "test:ci": "jest --ci --maxWorkers=4",
    "test:coverage:ci": "jest --ci --maxWorkers=4 --coverage --testResultsProcessor=\"jest-junit\"",
    "test:coverage:codecov": "codecov",
    "test:watch": "jest --watch",
    "ts:build": "tsc",
    "ts:watch": "tsc -w",
    "tslint": "tslint --config tslint.json \"src/**/*.ts\"",
    "tslint:ci": "tslint --config tslint.json --format junit --out reports/tslint/tslint-results.xml \"src/**/*.ts\"",
    "tslint:fix": "tslint --config tslint.json --fix \"src/**/*.ts\"",
    "verify": "yarn verify:npmvet && yarn verify:david && yarn verify:nsp && yarn verify:tscheck && yarn tslint && yarn test",
    "verify:offline": "yarn verify:npmvet && yarn tslint && yarn test",
    "verify:circleci": "circleci config validate -c .circleci/config.yml",
    "verify:ci": "yarn verify:npmvet && yarn verify:nsp && yarn test:ci",
    "verify:check": "depcheck",
    "verify:david": "david",
    "verify:npmvet": "npmvet --renderer ci --strict",
    "verify:nsp": "nsp check --output summary",
    "verify:tscheck": "types-checker"
  },
  "main": "./lib/status-board.js",
  "types": "./lib/status-board.d.ts",
  "repository": "git@github.com:jameswlane/status-board.git",
  "author": "James W. Lane <james.w.lane@mac.com>",
  "license": "MIT",
  "dependencies": {
    "async": "2.6.1",
    "body-parser": "1.18.3",
    "chalk": "2.4.1",
    "compression": "1.7.3",
    "css": "2.2.4",
    "debug": "4.1.0",
    "ejs": "2.6.1",
    "errorhandler": "1.5.0",
    "express": "4.16.4",
    "method-override": "3.0.0",
    "moment": "2.22.2",
    "morgan": "1.9.1",
    "nib": "1.1.2",
    "pg": "7.5.0",
    "read-package-json": "2.0.13",
    "request": "2.88.0",
    "semver": "5.6.0",
    "socket.io": "2.1.1",
    "stylus": "0.54.5",
    "tracer": "0.9.1",
    "traverse": "0.6.6",
    "underscore": "1.9.1",
    "winston": "3.1.0",
    "xtend": "4.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@types/async": "2.0.49",
    "@types/body-parser": "1.17.0",
    "@types/chance": "1.0.1",
    "@types/compression": "0.0.36",
    "@types/css": "0.0.31",
    "@types/debug": "0.0.31",
    "@types/ejs": "2.6.0",
    "@types/errorhandler": "0.0.32",
    "@types/express": "4.16.0",
    "@types/faker": "4.1.4",
    "@types/jest": "23.3.5",
    "@types/method-override": "0.0.31",
    "@types/morgan": "1.7.35",
    "@types/node": "10.0.7",
    "@types/pg": "7.4.11",
    "@types/request": "2.47.1",
    "@types/semver": "5.5.0",
    "@types/serve-favicon": "2.2.30",
    "@types/socket.io": "1.4.38",
    "@types/socket.io-client": "1.4.32",
    "@types/stylus": "0.48.30",
    "@types/traverse": "0.6.32",
    "@types/underscore": "1.8.9",
    "@types/winston": "2.4.4",
    "@types/xtend": "4.0.2",
    "all-contributors-cli": "5.4.0",
    "babel-core": "6.26.3",
    "babel-jest": "23.6.0",
    "chance": "1.0.16",
    "codecov": "3.1.0",
    "commitizen": "3.0.2",
    "coveralls": "3.0.2",
    "cz-conventional-changelog": "2.1.0",
    "faker": "4.1.0",
    "jest": "23.6.0",
    "jest-express": "1.8.0",
    "jest-junit": "5.2.0",
    "lint-staged": "7.3.0",
    "mock-socket": "8.0.4",
    "opt-cli": "1.6.0",
    "rimraf": "2.6.2",
    "semantic-release": "15.10.3",
    "socket.io-client": "2.1.1",
    "ts-jest": "23.10.4",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.11.0",
    "typescript": "3.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest-junit": {
    "output": "./ts-test-results.xml"
  },
  "tscheck": {
    "exclude": [
      "chalk",
      "moment"
    ]
  },
  "david": {
    "ignore": [
      "@types/node"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "@types/node"
    ],
    "commitMessages": {
      "initialBadge": ":memo: Docs: Add Greenkeeper badge",
      "initialDependencies": ":gem: Upgrade: Update dependencies",
      "initialBranches": ":tada: Build: Whitelist greenkeeper branches",
      "dependencyUpdate": ":gem: Upgrade: Update ${dependency} to version ${version}",
      "devDependencyUpdate": ":gem: Upgrade: Update ${dependency} to version ${version}",
      "dependencyPin": "fix(package): Updated ${dependency} to version ${version}",
      "devDependencyPin": "chore(package): Updated ${dependency} to version ${version}"
    }
  }
}
